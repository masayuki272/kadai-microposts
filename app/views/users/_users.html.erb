<!--フォロー、フォロワーの表示のパーシャル-->
<% if users.any? %>
  <ul class="media-list">
    <% users.each do |user| %>
    <li class ="media">
      <div class="media-left">
       <img class="media-object img-rounded" src="<%= gravatar_url(user, {size: 50}) %>" alt"">
       <!--Gravatar メールアドレスに対して自分のアバター画像を登録するサービス-->
      </div>
      <div class="media-body">
        <div>
          <%= user.name %>
        </div>
        <div>
          <p><%= link_to 'View profile', user_path(user) %></p>
        </div>
      </div>
    </li>
    <% end %>
  </ul>
  <%= paginate users %>
  <!--ページネーション-->
  <% end %>